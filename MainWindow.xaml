<Window x:Class="DiarioGeneral.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiarioGeneral"
        mc:Ignorable="d"
        Title="Ventana" Height="115" Width="379" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel>
            <Menu Height="20">
                
                <MenuItem Header="Diario General">
                    <MenuItem x:Name="DiarioMenuItem" Header="Diario" Click="DiarioMenuItem_Click"/>
                </MenuItem>

                <MenuItem Header="Balance De Comprobación">
                    <MenuItem x:Name="BalanceMenuItem" Header="Balance" Click="BalanceMenuItem_Click"/>
                </MenuItem>
                
            </Menu>
        </StackPanel>
    </Grid>
</Window>
